<section id="amb-approve">

  <h1><strong>Lists of users</strong></h1>
  <table>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
    </tr>

    <% users.forEach((user) => { %>
      <tr>
        <td class="id-dri"><%= user.userID %></td>
        <td class="name"><%= user.userName %></td>
        <td class="mail"><%= user.userEmail %></td>
